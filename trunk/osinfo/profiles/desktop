desktop_profile() {
		
	for module in system \
	              distro \
				  kernel \
				  processor \
				  memory \
				  hdd \
				  network \
				  wlan \
				  dmi \
				  applications \
				  terminal \
				  devices \
				  cdrom \
				  users \
				  services; do

		Module_${module}

	done
}

